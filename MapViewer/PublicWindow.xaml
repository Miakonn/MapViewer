<Window x:Class="MapViewer.PublicWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Public Maps" Height="600" Width="700" Closing="PublicWindow_Closing"
		 WindowStyle="None">
	<Border Name="PublicBorder" BorderThickness="0,0" >
		<Grid>
			<ContentPresenter Grid.Row="0" Grid.Column="0" Name="Layer1"  Margin="0" />
			<ContentPresenter Grid.Row="0" Grid.Column="0" Name="Layer3"  Margin="0" />
            <ContentPresenter Grid.Row="0" Grid.Column="0" Name="Layer4"  Margin="0" />
            <ContentPresenter Grid.Row="0" Grid.Column="0" Name="Layer5"  Margin="0" />
		</Grid>
	</Border>
</Window>
